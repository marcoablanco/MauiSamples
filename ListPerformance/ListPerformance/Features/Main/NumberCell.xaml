<?xml version="1.0" encoding="utf-8" ?>
<Grid xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	  x:Class="ListPerformance.Features.Main.NumberCell"
	  RowDefinitions="*,Auto,*" ColumnDefinitions="*,Auto,*"
	  Padding="10">

	<Label x:Name="LblProperty1"
		   Grid.Row="0" Grid.Column="0" />
	<Label x:Name="LblProperty2"
		   Grid.Row="0" Grid.Column="1" 
		   HorizontalTextAlignment="Center" />
	<Label x:Name="LblProperty3"
		   Grid.Row="0" Grid.Column="2" 
		   HorizontalTextAlignment="End" />

	<Label x:Name="LblProperty4"
		   Grid.Row="1" Grid.Column="0" />
	<Label x:Name="LblProperty5"
		   Grid.Row="1" Grid.Column="1" 
		   HorizontalTextAlignment="Center" />
	<Label x:Name="LblProperty6"
		   Grid.Row="1" Grid.Column="2" 
		   HorizontalTextAlignment="End" />

	<Label x:Name="LblProperty7"
		   Grid.Row="2" Grid.Column="0" />
	<Label x:Name="LblProperty8"
		   Grid.Row="2" Grid.Column="1" 
		   HorizontalTextAlignment="Center" />
	<Label x:Name="LblProperty9"
		   Grid.Row="2" Grid.Column="2" 
		   HorizontalTextAlignment="End" />
</Grid>
